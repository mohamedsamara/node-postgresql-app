{
  "name": "node-postgresql-app",
  "version": "1.0.0",
  "description": "Node PostgreSQL Application",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack && cross-env NODE_ENV=production babel -d ./dist/server server",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node server",
    "start:production": "cross-env NODE_ENV=production node index.js",
    "production": "npm run clean && npm run build",
    "lint": "eslint '**/*.{js,jsx}' --ignore-path .eslintignore",
    "lint:fix": "eslint '**/*.{js,jsx}' --fix --ignore-path .eslintignore",
    "test": "run-p test:*",
    "test:client": "cross-env NODE_ENV=test mocha --opts mocha.opts 'client/**/*.test.*' --watch",
    "test:server": "cross-env NODE_ENV=test mocha --opts mocha.opts 'server/**/*.test.*' --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/node-postgresql-app.git"
  },
  "keywords": [
    "node",
    "express",
    "sequelize",
    "postgresql",
    "react",
    "material-ui",
    "webpack",
    "eslint",
    "mocha",
    "chai",
    "sinon"
  ],
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedsamara/node-postgresql-app/issues"
  },
  "homepage": "https://github.com/mohamedsamara/node-postgresql-app#readme",
